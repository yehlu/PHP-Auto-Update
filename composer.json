    "minimum-stability": "stable",
    "require": {
        "ext-json": "*",
        "ext-curl": "*",
        "ext-zip": "*",
        "php": "^8.0 || ^8.1 || ^8.2 || ^8.3",
        "desarrolla2/cache": "^3.0",
        "monolog/monolog": "^2.0",
